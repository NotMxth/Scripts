-- Start (Presses the Key "E" every 0.015 Seconds)

while wait(0.25) do
keypress(0x45)
wait(0.015)
keyrelease(0x45)
end

-- End

-- Start (Checks every second if ur ability is on, if not activates it)
while wait(1) do
if game.Players.LocalPlayer.Character:FindFirstChild("ActiveAbility") == nil then
  local args = {
    [1] = true
}

game:GetService("ReplicatedStorage").ToggleAbility:InvokeServer(unpack(args))
    end
end

-- End

-- Start (Teleports the NPC infront of you)

while wait() do
game:GetService("Workspace").Umberhallow.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-6)
end

-- End
